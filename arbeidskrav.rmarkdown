```{r}
#| echo: false
# Laste inn datasett for rase
race <- read.dbf("MandD/acs_b02001_race.dbf") %>%
  select(-M02001001, -M02001002, -M02001003, -M02001004, -M02001005, -M02001006,
         -M02001007, -M02001008, -M02001009, -M02001010 )

